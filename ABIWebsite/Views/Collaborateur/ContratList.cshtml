@model IEnumerable<ABIModel.Contrat>

@{
    ViewBag.Title = "ContratList";
}

<h2>ContratList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SalaireBrut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateDebutContrat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateFinEffectif)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MotifCloture)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cloture)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SalaireBrut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateDebutContrat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateFinEffectif)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MotifCloture)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cloture)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
